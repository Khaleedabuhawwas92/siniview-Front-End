{
  "name": "com.webspace.electron_nuxt",
  "productName": "sineview",
  "version": "1.1.3",
  "description": "Electron application boilerplate based on Nuxt.js for crossplatform desktop application development",
  "author": {
    "name": "Webspace Team",
    "email": "info@webspaceteam.com",
    "url": "https://webspaceteam.com"
  },
  "private": true,
  "main": "main.js",
  "build": {
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/.nuxt/**",
      "!**/build/**",
      "!**/mainprocess/**",
      "!**/src/**",
      "!main.mainprocess.js",
      "!main.dev.js",
      "!main.dev.js.map",
      "!webpack.config.js",
      "!.gitignore",
      "!nuxt.config.js",
      "!package-lock.json",
      "!README.md"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "perMachine": true,
      "include": "build/installer.nsh"
    },
    "afterSign": "build/afterSignHook.js",
    "mac": {
      "target": [
        "dmg"
      ],
      "provisioningProfile": "build/embedded.provisionprofile",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist"
    },
    "mas": {
      "hardenedRuntime": false,
      "provisioningProfile": "build/embedded.provisionprofile",
      "entitlements": "build/entitlements.mas.plist",
      "entitlementsInherit": "build/entitlements.mas.inherit.plist",
      "gatekeeperAssess": false,
      "asarUnpack": []
    }
  },
  "scripts": {
    "dev": "concurrently --kill-others \"webpack --mode=development && electron ./main.dev.js\" ",
    "json:server": "json-server --watch db.json",
    "build:win:64": "nuxt build && webpack && electron-builder --win --x64",
    "build:win:32": "nuxt build && webpack && electron-builder --win --ia32",
    "build:win:both": "nuxt build && webpack && electron-builder --win --x32 --x64",
    "build:mac:dmg": "nuxt build && webpack && CSC_NAME=\"CERTIFICATE NAME (***********)\" appleId='apple@developer.id' appleIdPassword='application password' electron-builder",
    "build:mac:store": "nuxt build && webpack && CSC_NAME=\"CERTIFICATE NAME (***********)\" electron-builder && bash build/resignAndPackage.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webspaceteam/electron-nuxt.git"
  },
  "license": "MIT",
  "keywords": [
    "electron",
    "boilerplate",
    "nuxt",
    "nuxt.js",
    "vue",
    "vues",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "dependencies": {
    "-": "0.0.1",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/auth-next": "^5.0.0-1643791578.532b3d6",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/vuetify": "^1.12.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cctx": "^1.0.1",
    "child_process": "^1.0.2",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dateformat": "^5.0.2",
    "dotenv": "^10.0.0",
    "electron-serve": "^0.4.1",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "fs": "0.0.1-security",
    "g": "^2.0.1",
    "is-port-busy": "^1.0.5",
    "json-server": "^0.17.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.2",
    "net": "^1.0.2",
    "node-couchdb": "^1.3.0",
    "nodemon": "^2.0.15",
    "pouchdb": "^7.2.2",
    "rxjs": "^7.5.4",
    "serverless-express": "^2.0.12",
    "shelljs": "^0.8.5",
    "winston": "^3.3.3",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.8.0",
    "@nuxtjs/moment": "^1.6.1",
    "core-js": "^2.6.11",
    "cross-env": "^6.0.3",
    "electron": "^7.1.8",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^0.2.1",
    "electron-packager": "^15.4.0",
    "nuxt": "^2.11.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
